#include <bits/stdc++.h>

using namespace std;

#define ll          long long
#define FOR(i,a,b)  for (int i = a; i <= b; ++i)
#define FORD(i,a,b) for (int i = a; i >= b; --i)
#define FILE(name)  if (fopen(name".INP", "r")){freopen(name".INP", "r", stdin); freopen(name".OUT", "w", stdout);}

template <class X, class Y>
    bool maximize(X &x, const Y &y){return (x < y) ? x = y, 1 : 0;}

template <class X, class Y>
    bool minimize(X &x, const Y &y){return (x > y) ? x = y, 1 : 0;}

const int MAXN = 1e5 + 5;
int n,q;
pair<ll,ll> a[MAXN];
ll candies[MAXN];
ll k;

signed main(void)
{
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr); cout.tie(nullptr);
    FILE("");

    cin >> n >> q;
    FOR(i,1,n) cin >> a[i].first >> a[i].second;

    sort(a+1,a+n+1,[&](const pair<ll,ll> &x, const pair<ll,ll> &y)
         {
            return x.second < y.second;
         });

    FOR(i,1,n) candies[i] = candies[i-1] + a[i].first;
    candies[n+1] = 1e15;

    while (q--)
    {
        cin >> k;
        int pos = lower_bound(candies+1,candies+n+2,k)-candies;
        cout << a[pos].second << '\n';
    }

    return 0;
}
